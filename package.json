{
  "name": "doz-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node script/development.js",
    "build": "parcel build public/index.html --no-cache --no-source-maps",
    "snap": "npm run-script build && doz-snap -f dist/index.html",
    "test": "mocha -r ignore-styles -r @babel/register \"./{,!(node_modules)/**}/*.test.js\"",
    "server": "node server/app.js",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm run-script build && git add dist && git commit -a -m BUILD",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "doz-ssr": "latest",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3"
  },
  "devDependencies": {
    "@abide-community/parcel-plugin-clean-out-dir": "^1.0.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "autoprefixer": "^9.1.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "caniuse-lite": "^1.0.30001010",
    "chai": "^4.1.2",
    "doz": "latest",
    "doz-hot-location-reload": "latest",
    "doz-metatag": "latest",
    "doz-router": "latest",
    "doz-snap": "latest",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "open": "^7.0.0",
    "parcel-bundler": "1.12.5",
    "postcss-modules": "^1.3.2",
    "regenerator-runtime": "^0.13.5"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "client/*",
      ".idea/*"
    ],
    "env": {
      "NODE_ENV": "development",
      "PORT": "3000"
    }
  }
}
