{
  "name": "doz-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "start": "parcel serve public/index.html --no-cache --no-source-maps",
    "build": "parcel build public/index.html --no-cache --no-source-maps",
    "snap": "npm run-script build && doz-snap -f dist/index.html",
    "test": "mocha -r ignore-styles -r @babel/register \"./{,!(node_modules)/**}/*.test.js\"",
    "server": "node server/app.js",
    "release:major": "npm version major",
    "release:minor": "npm version minor",
    "release:patch": "npm version patch",
    "preversion": "npm run-script build && git add dist && git commit -a -m BUILD",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "doz-ssr": "^2.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "normalize-url": "^4.5.1",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@parcel/core": "^2.2.1",
    "@parcel/packager-raw-url": "^2.2.1",
    "@parcel/transformer-webmanifest": "^2.2.1",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "doz": "^3.13.10",
    "doz-hot-location-reload": "latest",
    "doz-metatag": "latest",
    "doz-router": "latest",
    "doz-snap": "latest",
    "ignore-styles": "^5.0.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.0",
    "parcel": "^2.2.1",
    "postcss": "^8.4.6",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2"
  }
}
